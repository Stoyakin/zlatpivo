html,
body
  font-family $os
body
  overflow-x hidden
  &:not(.pace-done)
    overflow hidden
  &
    .header,
    .content,
    .footer
      opacity 0
      transition .7s opacity
    .content
      z-index -5
      pos relative
      transition-delay 0.3s
    .header,
    .footer
      transition-delay 0.8s
  &.pace-done
    .header,
    .content,
    .footer
      opacity 1
    .content
      z-index 1
      
.container
  w 100%
  max-width 1440px
  padding 0 87px
  margin 0 auto
  pos relative
  z-index 1
  @media $lDesktopSmall
    max-width 100%
  @media $desktopMedium
    max-width 100%
    padding 0 47px
  @media $LandscapeMedium
    padding 0 20px

body.index
  .container
    @media $lDesktopSmall
      max-width calc(100% - 100px)

.index .content
  pt 0
  pb 0
  bgc $blue

body:not(.index) .content
  background-position bottom -630px center
  background-repeat no-repeat

.content
  pt 217px
  pb 96px
  bgc $cream
  @media $LandscapeMedium
    pt 180px
  &__video
    position fixed
    z-index -1
    t 50%
    l 50%
    min-height 100%
    min-width 100%
    transform translate(-50%, -50%)
    opacity .7
  h1:not([class])
    color black
    fontify 72px 700 76px
    margin 63px 0 35px
  h2:not([class])
    color black
    fontify 48px 700 54px
    margin 63px 0 28px
  h3:not([class])
    color white
    fontify 36px 700 42px
    margin 65px 0 38px
  h4:not([class])
    color white
    fontify 24px 700 34px
    margin 43px 0 16px
  img:not([class])
    margin 5px 0
    max-width 100%
  ul:not([class])
    list-style  none
    margin  23px 0
  ul:not([class]) li
    pl 30px
    pos relative
    fontify 20px 400 27px
  ul:not([class]) li + li
    mt 25px
  ul:not([class]) li:before
    content ""
    pos absolute
    l 0
    t 13px
    size 14px 1px
    bgc white
  ol:not([class])
    list-style none
    margin 23px 0
    counter-reset myCounter
  ol:not([class]) li
    pl 30px
    pos relative
    fontify 18px 400 24px
  ol:not([class]) li + li
    mt 24px
  ol:not([class]) li:before
    counter-increment myCounter
    content counter(myCounter)"."	
    pos absolute
    l 0
    t 0
    w 30px
    pr 9px
    color $green
    fontify 18px 400 24px
    @media $lDesktopMedium
      fontify 15px 400 23px
  p:not([class])
    color white
    margin 25px 0
    fontify 16px 300 24px
    @media $LandscapeMax
      fontify 14px 300 24px
  a:not([class])
    color white
    border-bottom 2px solid $orange
    transition .5s border
    &:hover
      border-color transparent
 
  *:not([class]):not(span):first-child
    mt 0 !important
  *:not([class]):not(span):last-child
    margin-bottom 0 !important

  table
    margin 30px 0
    border 2px solid $green
  table th
    text-align left
    padding 16px 24px
    color black
    fontify 15px 400 21px
    border 1px solid #f0f3e1
  table td
    padding 18px 24px
    color white
    fontify 15px 400 21px
    border 1px solid #f0f3e1

  &__dec1
    filter brightness(0%)
    l calc(50% - 32px)
    t -4%
  &__dec2
    filter brightness(0%)
    l -54px
    t 31%
    transform scale(.8) rotate(-38deg)
  &__dec3
    filter brightness(0%)
    r 35%
    t 58%
    transform scaleX(-1)

input::-webkit-input-placeholder
  color alpha(white, .5)
input::-moz-placeholder
  color alpha(white, .5)
input:-ms-input-placeholder
  color alpha(white, .5)
  
div[class\^="table-scroller"]
  overflow auto
  pos relative
  @media $desktopMax
    w 100%

.btn
  ib()
  bgc $blue
  border 0
  border-radius 5px
  color white
  outline none
  cursor pointer
  fontify 14px 400 36px
  padding 0 15px
  transition .7s background, .7s color
  &:hover
    bgc #e8ebf5
    color #7ca8cb
    
.common
  &__title
    fontify 50px 600 52px
    mb 52px
    ml -3px
    @media $LandscapeMedium
      fontify 30px 600 32px
      ml -2px
  &__pad
    pl 87px
    pr 87px
    @media $desktopMedium
      pl 47px
      pr 47px
    @media $LandscapeMedium
      pl 20px
      pr 20px
  &__href
    pos relative
    ib()
    mr 21px
    color $dark
    fontify 15px 600 24px
    border-bottom 2px solid $orange
    transition .7s border
    @media $desktopMax
      fontify 14px 600 24px
    &_white
      color white
    &:before
      content ""
      pos absolute
      r -22px
      t 10px
      size 13px 8px
      background url('data:image/png;base64,') no-repeat center
      transition .7s right
    &:hover
      border-color transparent
  &__input
    w 100%
    h 38px
    border 0
    border-bottom 2px solid white
    bgc transparent
    color white
    font-size 14px
    font-family $os
    line-height 400
    transition .5s border
    &:focus + ^[0]__placeholder,
    &.notempty + ^[0]__placeholder
      fontify 11px 400 11px
      t -12px
  &__textarea
    w 100%
    h 188px
    overflow hidden
    resize none
    padding 5px 0
    border 0
    border-bottom 2px solid #ececec
    bgc transparent
    color white
    font-size 14px
    font-family $os
    transition .5s border
    @media $PortraitMax
      h 100px
    &:focus + ^[0]__placeholder,
    &.notempty + ^[0]__placeholder
      fontify 11px 400 11px
      t -12px
  &__select
    h 36px
    bgc #fff
    border 2px solid #b6b6b6
    w auto
    cursor pointer
    transition .5s border
    &.warning
      border-color #ed1c24
  &__placeholder
    pos absolute
    z-index -1
    l 0 
    t 0
    color alpha(white, .5)
    fontify 14px 400 36px
    text-overflow ellipsis
    white-space nowrap
    overflow hidden
    max-width 100%
    transition .7s top, .7s font-size
    @media $desktopMax
      font-size 14px
  &__col
    pos relative
    w calc(50% - 18px)
    @media $desktopMax
      w calc(50% - 15px)
    @media $PortraitMax
      w 100%
      & + &
        mt 30px
  &__row
    display flex
    justify-content space-between
    align-items flex-start
    pos relative
    z-index 1
    w 100%
    @media $PortraitMax
      flex-direction column
    & + &
      mt 37px
      @media $PortraitMax
        mt 30px
  &__fileover
    w 100%
    pos relative
    pt 14px
    pb 14px
    border-bottom 1px solid white
    transition .5s border
    &.warning
      border-color #ed1c24
    & + &
      mt 20px
  &__file
    pos absolute
    w 0
    h 0
    l 0
    t 0
    opacity 0
    &name
      ib()
      pos relative
      l 26px
      cursor pointer
      color white
      fontify 14px 700 14px
      letter-spacing .1em
      text-transform uppercase
      border-bottom 1px solid alpha(white, .25)
      transition .5s border
      @media $lDesktopSmall
        font-size 12px
      &.choosed,
      &:hover
        border-color transparent
      &:after
        content "*"
        pos absolute
        r -11px
        fontify 23px 400 14px
        t -3px
      &:before
        content ""
        pos absolute
        l -24px
        t 3px
        size 16px 8px
        background url("data:image/png;base64,") no-repeat center
      &.choosed + ^[-1]delete
        visibility visible
        cursor pointer
      &.choosed:after
        content ""
    &delete
      ib()
      visibility hidden
      mt 3px
      ml 32px
      size 9px
      background url("data:image/png;base64,") no-repeat center
      
.fakecheckbox
  ib()
  pos relative
  &__label
    ib()
    color white
    fontify 14px 400 24px
    pos relative
    pl 29px
    pt 7px
    &:before 
      content ""
      pos absolute
      l 0px
      t 8px
      size 16px
      border 2px solid #ececec
      cursor pointer
      transition .5s border
    &:after
      content ""
      pos absolute
      size 14px 10px
      background url('data:image/png;base64,') no-repeat center
      l 4px
      t 14px
      opacity 0
      cursor pointer
      transition .5s opacity
  &__truecheck
    pos absolute
    l 0
    t 0
    w 0
    h 0
    border 0
    opacity 0
    &:checked + ^[0]__label:after
      opacity 1
    &.warning + ^[0]__label:before 
      border-color #ed1c24
    
.totop
  pos fixed
  z-index 5
  b 10%
  r 0
  border-top-left-radius 18px
  border-bottom-left-radius 18px
  cursor pointer
  size 34px
  visibility hidden
  opacity 0
  background url('data:image/png;base64,') no-repeat 16px center $orange
  transition .5s visible, .5s opacity
  @media $LandscapeMax
    dn()
  &.visible
    visibility visible
    opacity 1
      
.breadcrumbs
  color #4f4f4f
  mb 18px
  &__href
    color #4f4f4f
    fontify 14px 400 18px
    border-bottom 1px solid #4f4f4f
    transition .5s border
    &:hover
      border-color transparent
  &__delim
    padding 0 5px
    fontify 14px 400 18px

.bgfixed
  background-position center
  background-attachment fixed
  background-size cover
      
input.warning,
textarea.warning
  border-color $orange

.hidden-block
  display none
  
.decor
  pos absolute
  opacity .2
  background-position center
  background-repeat no-repeat
  @media $LandscapeMax
    dn()
  &_type1
    size 225px 228px
    background-image url('../i/dec1.png')
  &_type2
    size 159px 98px
    background-image url('../i/dec2.png')
  &_type3
    size 182px 338px
    background-image url('../i/dec3.png')
  &_type4
    size 281px 300px
    background-image url('../i/dec4.png')
  &_type5
    size 99px 152px
    background-image url('../i/dec5.png')
  &_type6
    size 237px 284px
    background-image url('../i/dec6.png')
  &_type7
    size 188px 254px
    background-image url('../i/dec7.png')
  &_type8
    opacity .04
    size 344px 196px
    background-image url('../i/dec8.png')
  &_type9
    size 245px 230px
    background-image url('../i/dec9.png')
  &_type10
    size 233px 269px
    background-image url('../i/dec10.png')

.tippy-tooltip.light-theme
  bgc white
  box-shadow 0 6px 32px rgba(0, 0, 0, 0.21)
  fontif 14px 400 18px
  color white
  padding 15px 19px
  .tippy-backdrop
    bgc white
    color white
  .tippy-arrow
    border-top-color white