.socials
  pos absolute
  z-index 2
  r 60px
  t 60px
  df()
  @media $desktopMax
    r 50px
    t 50px
  @media $LandscapeMax
    t 35px
    r 95px
  @media $PortraitMedium
    r 75px
  @media $PortraitMin
    dn()
  &__href
    size 26px
    border-radius 50%
    transition .7s background
    & + &
      ml 10px
    &[data-type="vk"]
      background url('data:image/png;base64,') no-repeat center
      &:hover
        background-image url('data:image/png;base64,')
    &[data-type="inst"]
      background url('data:image/png;base64,') no-repeat center
      &:hover
        background-image url('data:image/png;base64,')
    &[data-type="fb"]
      background url('data:image/png;base64,') no-repeat center
      &:hover
        background-image url('data:image/png;base64,')
    

.header
  pos absolute
  z-index 2
  l 0
  w calc((100% - 1438px) / 2)
  border-right 1px solid rgba(255, 255, 255, .1)
  padding 61px 60px 30px
  h 100vh
  @media $lDesktopMax
    w 200px
    padding 40px 50px 30px
  @media $desktopMax
    w 195px
  @media $LandscapeMax
    l -195px
  @media $LandscapeMax
    pt 20px
  &__logo
    db()
    pos relative
    l -12px
    mb 92px
    @media $lDesktopMax
      mb 68px
      l -11px
      w 120px
    @media $LandscapeMax
      l 174px
      w 100px
    &-img
      max-width 100%
  &__shadow
    pos fixed
    z-index 12
    b 0
    h 0
    w 100%
    l 0
    bgc #3c2c24
    transition .7s height
  &__button
    dn()
    pos fixed
    r 35px
    t 35px
    w 25px
    h 25px
    z-index 15
    @media $LandscapeMax
      db()
    &:before
      content ""
      pos absolute
      t 0
      r 0
      w 25px
      h 2px
      bgc $orange
      transition .5s width, .5s background, .5s transform, .5s top
    &:after
      content ""
      pos absolute
      b 0
      r 0
      w 25px
      h 2px
      bgc $orange
      transition .5s width, .5s background, .5s transform, .5s top  
    &-meat
      content ""
      pos absolute
      t 50%
      r 0
      w 25px
      h 2px
      mt -1px
      bgc $orange
      transition .5s width, .5s background, .5s transform, .5s top  
    &:not(.open):hover
      &:before
        animation burger-hover 1s infinite ease-in-out alternate
      &:after
        animation burger-hover 1s infinite ease-in-out alternate forwards 400ms
    &:not(.open):hover &-meat
      animation burger-hover 1s infinite ease-in-out alternate forwards 200ms
    &.open
      &:before
        animation burger-top 1s ease-in-out forwards
      &:after
        animation burger-bottom 1s ease-in-out forwards
      & ^[-1]-meat
        opacity 0
    &.open.remove
      &:before
        animation burger-top-invert 1s ease-in-out forwards
      &:after
        animation burger-bottom-invert 1s ease-in-out forwards
      & ^[-1]-meat
        opacity 1
        transition .4s opacity .6s
        
        
@keyframes burger-hover
  0%
    w 100%
  50%
    w 50%
  100%
    w 100%
      
@keyframes burger-top
  0%
    t 0
  40%
    t 11px
  60%
    t 11px
    transform rotate(0)
  100%
    t 11px
    transform rotate(45deg)
    
@keyframes burger-bottom
  0%
    b 0
  40%
    b 12px
  60%
    b 12px
    transform rotate(0)
  100%
    b 12px
    transform rotate(-45deg)

@keyframes burger-top-invert
  0%
    t 12px
    transform rotate(45deg)
  40%
    t 12px
    transform rotate(0)
  60%
    t 12px
  100%
    t 0
    
@keyframes burger-bottom-invert
  0%
    b 12px
    transform rotate(-45deg)
  40%
    b 12px
    transform rotate(0)
  60%
    b 12px
  100%
    b 0
    
    
html.burgeropen
  overflow hidden
  body
    overflow hidden
  .header
    &__shadow
      h 100%
    .menu
      l 0
      opacity 1
      transition-delay .7s
    
html.touchevents
  .header__button
    &:not(.open):hover
      &:before
        animation none
      .header__button-meat
        animation none
      &:after
        animation none
    &:not(.open):hover .burger__button-meat
      animation none